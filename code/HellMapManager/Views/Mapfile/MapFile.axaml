  xmlns:rooms="clr-namespace:HellMapManager.Views.Mapfile.Rooms"
  xmlns:vm="using:HellMapManager.ViewModels"
  d:DesignHeight="450"
  d:DesignWidth="800"
  x:DataType="vm:MainWindowViewModel"
  mc:Ignorable="d">
