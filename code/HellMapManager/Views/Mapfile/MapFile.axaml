      </TabItem>
      <TabItem Header="定位">
        <TextBlock Margin="5">This is tab 3 content</TextBlock>
      </TabItem>
      <TabItem Header="捷径">
        <TextBlock Margin="5">This is tab 3 content</TextBlock>
      </TabItem>
      <TabItem Header="变量">
        <TextBlock Margin="5">This is tab 3 content</TextBlock>
      </TabItem>
      <TabItem Header="快照">
        <TextBlock Margin="5">This is tab 3 content</TextBlock>
      </TabItem>
    </TabControl>
  </DockPanel>
</UserControl>
