<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             xmlns:vm="using:HellMapManager.ViewModels"
             x:DataType="vm:MainWindowViewModel"
             x:Class="HellMapManager.Views.Mapfile.Overview.Overview">
  <StackPanel Margin="20">
  <TextBlock Text="地图名:"/>
  <TextBlock Text="{Binding GetMap.Map.Info.Name}"/>
  <TextBlock Text="地图描述:"/>
  <TextBlock Text="{Binding GetMap.Map.Info.Desc}"/>
  <TextBlock Text="地图房间数量:"/>
  <TextBlock Text="{Binding GetMap.Map.Rooms.Count}"/>

  </StackPanel>
</UserControl>
