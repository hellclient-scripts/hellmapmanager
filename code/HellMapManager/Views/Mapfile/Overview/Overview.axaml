<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             xmlns:vm="using:HellMapManager.ViewModels"
             x:DataType="vm:MainWindowViewModel"
             x:Class="HellMapManager.Views.Mapfile.Overview.Overview">
  <StackPanel Margin="20">
  <TextBlock Text="地图名:"/>
  <TextBlock Text="{Binding GetMap.Map.Info.NameLabel}"/>
  <TextBlock Text="描述:"/>
  <TextBlock Text="{Binding GetMap.Map.Info.DescLabel}"/>
  <TextBlock Text="房间数量:"/>
  <TextBlock Text="{Binding GetMap.Map.Rooms.Count}"/>
  <TextBlock Text="别名数量:"/>
  <TextBlock Text="{Binding GetMap.Map.Aliases.Count}"/>
  <TextBlock Text="线数量:"/>
  <TextBlock Text="{Binding GetMap.Map.Routes.Count}"/>
  <TextBlock Text="变量数量:"/>
  <TextBlock Text="{Binding GetMap.Map.Variables.Count}"/>
  </StackPanel>
</UserControl>
